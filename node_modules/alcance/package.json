{
  "name": "alcance",
  "version": "0.16.0",
  "description": "Alcance - Design System Acessível",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "unpkg": "dist/alc/alc.js",
  "collection:main": "dist/collection/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/",
    "loader/",
    "utils/"
  ],
  "watch": {
    "update.globalstyles": {
      "patterns": [
        "src/global/styles/",
        "src/pages/",
        "src/components"
      ],
      "extensions": "scss,html,tsx",
      "quiet": false
    }
  },
  "scripts": {
    "watch": "npm-watch",
    "update.globalstyles": "node scripts/touch-global-scss.mjs",
    "icons": "node scripts/atualiza-icones.mjs",
    "build": "node scripts/atualiza-icones.mjs && echo 'Ícones atualizados' && stencil build",
    "build.watch": "node scripts/atualiza-icones.mjs && stencil build --watch",
    "start.devserver": "node scripts/atualiza-icones.mjs && stencil build --dev --watch --serve",
    "start": "run-p start.devserver watch",
    "test": "stencil test --spec --e2e",
    "test.spec": "stencil test --spec --watch",
    "test.e2e": "stencil test --e2e --watch",
    "test.watch": "stencil test --spec --e2e --watch",
    "test.watch.silent": "stencil test --spec --e2e --watch --silent",
    "teste.visual": "stencil test --e2e --screenshot",
    "generate": "stencil generate",
    "manifest": "cem analyze --config scripts/elements-manifest.cfg.mjs",
    "elements": "wca analyze src/**/*.tsx --format json --outFiles storybook-elements.json",
    "storybook": "node src/stories/process.assets.js && storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@babel/core": "^7.22.9",
    "@stencil/core": "^4.28.2",
    "@storybook/components": "^8.4.7",
    "@storybook/preview-api": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "framer-motion": "^11.2.12",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.5.0",
    "moment": "^2.29.4",
    "postcss-nesting": "^12.0.1",
    "prismjs": "^1.29.0",
    "react-code-blocks": "^0.1.6",
    "react-i18next": "^14.1.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@chromatic-com/storybook": "^3.2.2",
    "@custom-elements-manifest/analyzer": "^0.8.4",
    "@etchteam/storybook-addon-status": "^5.0.0",
    "@floating-ui/dom": "^1.5.3",
    "@shoelace-style/shoelace": "^2.0.0",
    "@stencil/postcss": "^2.1.0",
    "@stencil/react-output-target": "^0.5.3",
    "@stencil/sass": "^3.0.11",
    "@stencil/vue-output-target": "^0.8.8",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-controls": "^8.4.7",
    "@storybook/addon-docs": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-mdx-gfm": "^8.4.7",
    "@storybook/addon-storysource": "^8.4.7",
    "@storybook/addon-themes": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/html-vite": "^8.4.7",
    "@storybook/icons": "^1.3.0",
    "@storybook/manager-api": "^8.4.7",
    "@storybook/theming": "^8.4.7",
    "@types/jest": "^27.0.3",
    "@types/jquery": "^3.5.14",
    "@types/node": "^20.4.8",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^18.3.3",
    "@types/trusted-types": "^2.0.3",
    "autoprefixer": "^10.4.10",
    "chalk": "^5.0.1",
    "classnames": "^2.3.2",
    "command-line-args": "^5.2.1",
    "datatables.net": "~1.12.1",
    "datatables.net-plugins": "^1.13.5",
    "datatables.net-select": "~1.4.0",
    "del": "8.0.0",
    "download": "8.0.0",
    "front-matter": "4.0.2",
    "fs-extra": "^10.1.0",
    "glob-promise": "^5.0.0",
    "globby": "14.0.2",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jquery": "^3.7.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "0.13.0",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "prettier": "^3.0.3",
    "prism-react-renderer": "^2.4.0",
    "puppeteer": "^24.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recursive-copy": "2.0.14",
    "remark-gfm": "^3.0.1",
    "semver": "^7.5.0",
    "storybook": "^8.4.7",
    "storybook-addon-tag-badges": "^1.3.2",
    "storybook-react-i18next": "^3.1.8",
    "styled-components": "^6.1.13",
    "svgson": "^5.2.1",
    "tailwindcss": "^3.4.14",
    "touch": "^3.1.0",
    "vite": "^5.4.10"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://hub.camara.gov.br/repository/npm-hosted/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CamaraDosDeputados/alcance.git"
  },
  "keywords": [
    "typescript",
    "design system",
    "javascript",
    "seuso"
  ],
  "author": "Câmara dos Deputados",
  "bugs": {
